%description:
This test checks the correctness of dimensional signal power representation with linear interpolation.

%includes:
#include "inet/common/math/Function.h"
#include "inet/physicallayer/base/packetlevel/DimensionalTransmitterBase.h"

%global:

using namespace inet;
using namespace inet::physicallayer;
using namespace inet::units::values;

class DimensionalTransmitterTest : public DimensionalTransmitterBase
{
  public:
    void printValue(Function<W, s> *mapping, simtime_t time) {
        std::cout << "Value at " << time << ": " << mapping->at(Point<s>(s(time.dbl()))) << std::endl;
    }

    void printValue(Function<W, Hz> *mapping, Hz frequency) {
        std::cout << "Value at " << frequency << ": " << mapping->at(Point<simtime_t, Hz>(frequency)) << std::endl;
    }

    void printValue(Function<W, simtime_t, Hz> *mapping, simtime_t time, Hz frequency) {
        std::cout << "Value at " << time << " " << frequency << ": " << mapping->at(Point<simtime_t, Hz>(s(time.dbl()), frequency)) << std::endl;
    }

    void testTime1();
    void testTime2();
    void testFrequency1();
    void testFrequency2();
    void testDual1();
    void testDual2();
    void testDual3();
    void testDivide();
};

void DimensionalTransmitterTest::testTime1()
{
    timeGains.clear();
    timeGains.push_back(TimeGainEntry('%', 0, 1));
    timeGains.push_back(TimeGainEntry('%', 1, 1));
    auto p = createPowerMapping(10, 30, GHz(1), MHz(200), W(1));
    std::cout << *p << std::endl;
    for (int i = 0; i <= 45; i += 5)
        printValue(p, i);
}

void DimensionalTransmitterTest::testTime2()
{
    timeGains.clear();
    timeGains.push_back(TimeGainEntry('%', 0, 1));
    timeGains.push_back(TimeGainEntry('%', 1, 1));
    auto p1 = createPowerMapping(10, 30, GHz(1), MHz(200), W(1));
    auto p2 = createPowerMapping(20, 40, GHz(1), MHz(200), W(10));
    std::cout << *p1 << std::endl;
    std::cout << *p2 << std::endl;
    auto total = add(p1, p2);
    std::cout << *total << std::endl;
    for (int i = 0; i <= 45; i += 5)
        printValue(total, i);
    std::cout << "Max value: " << total->max() << std::endl;
}

void DimensionalTransmitterTest::testFrequency1()
{
    frequencyGains.clear();
    frequencyGains.push_back(FrequencyGainEntry('%', 0, 1));
    frequencyGains.push_back(FrequencyGainEntry('%', 1, 1));
    auto p = createPowerMapping(10, 30, GHz(1), MHz(200), W(1));
    std::cout << *p << std::endl;
    for (int i = 800; i <= 1250; i += 50)
        printValue(p, MHz(i));
}

void DimensionalTransmitterTest::testFrequency2()
{
    frequencyGains.clear();
    frequencyGains.push_back(FrequencyGainEntry('%', 0, 1));
    frequencyGains.push_back(FrequencyGainEntry('%', 1, 1));
    auto p1 = createPowerMapping(10, 30, GHz(1), MHz(200), W(1));
    auto p2 = createPowerMapping(10, 30, GHz(1.1), MHz(200), W(10));
    std::cout << *p1 << std::endl;
    std::cout << *p2 << std::endl;
    auto total = add(p1, p2);
    std::cout << *total << std::endl;
    for (int i = 800; i <= 1250; i += 50)
        printValue(total, MHz(i));
    std::cout << "Max value: " << total->max() << std::endl;
}

void DimensionalTransmitterTest::testDual1()
{
    timeGains.clear();
    timeGains.push_back(TimeGainEntry('%', 0, 1));
    timeGains.push_back(TimeGainEntry('%', 1, 1));
    frequencyGains.clear();
    frequencyGains.push_back(FrequencyGainEntry('%', 0, 1));
    frequencyGains.push_back(FrequencyGainEntry('%', 1, 1));
    auto p = createPowerMapping(10, 30, GHz(1), MHz(200), W(1));
    std::cout << *p << std::endl;
    for (int i = 0; i <= 45; i += 5)
        for (int j = 800; j <= 1250; j += 50)
            printValue(p, i, MHz(j));
}

void DimensionalTransmitterTest::testDual2()
{
    timeGains.clear();
    timeGains.push_back(TimeGainEntry('%', 0, 1));
    timeGains.push_back(TimeGainEntry('%', 1, 1));
    frequencyGains.clear();
    frequencyGains.push_back(FrequencyGainEntry('%', 0, 1));
    frequencyGains.push_back(FrequencyGainEntry('%', 1, 1));
    auto p1 = createPowerMapping(10, 30, GHz(1), MHz(200), W(1));
    auto p2 = createPowerMapping(20, 40, GHz(1.1), MHz(200), W(10));
    std::cout << *p1 << std::endl;
    std::cout << *p2 << std::endl;
    auto total = add(p1, p2);
    std::cout << *total << std::endl;
    for (int i = 0; i <= 45; i += 5)
        for (int j = 800; j <= 1250; j += 50)
            printValue(total, i, MHz(j));
    std::cout << "Max value: " << total->max() << std::endl;
}

void DimensionalTransmitterTest::testDual3()
{
    timeGains.clear();
    timeGains.push_back(TimeGainEntry('%', 0, 1));
    timeGains.push_back(TimeGainEntry('%', 1, 1));
    frequencyGains.clear();
    frequencyGains.push_back(FrequencyGainEntry('%', 0, 1));
    frequencyGains.push_back(FrequencyGainEntry('%', 1, 1));
    auto p1 = createPowerMapping(20, 30, GHz(1.1), MHz(400), W(1));
    auto p2 = createPowerMapping(10, 40, GHz(1.1), MHz(200), W(10));
    std::cout << *p1 << std::endl;
    std::cout << *p2 << std::endl;
    auto total = add(p1, p2);
    std::cout << *total << std::endl;
    for (int i = 0; i <= 45; i += 5)
        for (int j = 800; j <= 1350; j += 50)
            printValue(total, i, MHz(j));
    std::cout << "Max value: " << total->max() << std::endl;
}

void DimensionalTransmitterTest::testDivide()
{
    timeGains.clear();
    timeGains.push_back(TimeGainEntry('%', 0, 1));
    timeGains.push_back(TimeGainEntry('%', 0.5, 0.5));
    timeGains.push_back(TimeGainEntry('%', 1, 1));
    auto p1 = createPowerMapping(10, 30, GHz(1), MHz(200), W(1));
    auto p2 = createPowerMapping(20, 40, GHz(1), MHz(200), W(10));
    std::cout << *p1 << std::endl;
    std::cout << *p2 << std::endl;
    auto result = divide(p1, p2);
    std::cout << *result << std::endl;
    for (int i = 0; i <= 50; i += 1)
        printValue(result, i);
}

%activity:
DimensionalTransmitterTest tester;
tester.testTime1();
tester.testTime2();
tester.testFrequency1();
tester.testFrequency2();
tester.testDual1();
tester.testDual2();
tester.testDual3();
tester.testDivide();

